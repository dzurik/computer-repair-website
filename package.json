{
  "name": "informatique_domestique",
  "version": "1.0.0",
  "description": "Landing page informatique domestique",
  "browserslist": "last 10 versions",
  "main": "index.js",
  "scripts": {
    "watch:css": "node-sass css/main.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:css",
    "prefix:css": "postcss css/main.css --use autoprefixer -o css/style.css",
    "build:css": "npm-run-all prefix:css"
  },
  "author": "Oliver Dzurik",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.1.0"
  }
}
